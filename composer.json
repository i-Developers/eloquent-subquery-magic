{
  "name": "maksimru/eloquent-subquery-magic",
  "type": "library",
  "description": "Eloquent extension providing ability to use a lot of subquery functions like fromSubquery or leftJoinSubquery",
  "keywords": [
    "subquery",
    "eloquent",
    "laravel",
    "magic"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Martianov",
      "email": "maxmtmn@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5|~6.0",
    "fzaninotto/faker": "~1.7",
    "friendsofphp/php-cs-fixer": "^2.15",
    "nunomaduro/phpinsights": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "MaksimM\\SubqueryMagic\\": "src/SubqueryMagic"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "test/stubs/"
    }
  }
}
